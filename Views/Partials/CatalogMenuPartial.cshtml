@model List<ProductGroup>?

<div class="side-toggler">
  <div class="btn-anim">
    <a class="btn btn-main" href="#menu_links">КАТАЛОГ</a>
  </div>
</div>
<div id="menu_links">
  <h3 class="text-center position-relative">КАТАЛОГ<i class="position-absolute fas fa-times inline-icons mx-2"></i></h3>
  <ul class="">
    @if (Model != null)
      foreach (ProductGroup str in Model)
      {
        <li>
          <div class="btn-anim loader">
            <a class="btn btn-main" href="@Url.ActionLink("Category", "Home", new { id = str.id})">@str.name</a>
          </div>
        </li>
      }
  </ul>
</div>



